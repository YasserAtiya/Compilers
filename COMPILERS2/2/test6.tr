.fp 5 CBI
.ft CR
.nf
\f(CR 
main() 
{
    \f(CBint\f(CR array[\f5100\f(CR], n, c;
 
    printf(\fB"Enter the number of elements in array\\n"\f(CR);
    scanf(\fB"%d"\f(CR, &n);
 
    printf(\fB"Enter %d elements\\n"\f(CR, n);
 
    \f(CBfor\f(CR ( c = \f50\f(CR ; c < n ; c++ ) 
        scanf(\fB"%d"\f(CR, &array[c]);
 
    printf(\fB"Array elements entered by you are:\\n"\f(CR);
 
    \f(CBfor\f(CR ( c = \f50\f(CR ; c < n ; c++ ) 
        printf(\fB"array[%d] = %d\\n"\f(CR, c, array[c]);
 
    \f(CBreturn\f(CR \f50\f(CR;
}
