
.fp 5 CBI
.ft CR
.nf
\f(CBstruct\f(CR programming
{
    \f(CBfloat\f(CR constant;
    \f(CBchar\f(CR *pointer;
};
 
main()
{
   \f(CBstruct\f(CR programming variable;
   \f(CBchar\f(CR string[] = \fB"Programming in Software Development."\f(CR;   
 
   variable.constant = \f51\f(CR.\f523\f(CR;
   variable.pointer = string;
 
   printf(\fB"%f\\n"\f(CR, variable.constant);
   printf(\fB"%s\\n"\f(CR, variable.pointer);
 
   \f(CBreturn\f(CR \f50\f(CR;
}
