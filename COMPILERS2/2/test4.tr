.fp 5 CBI
.ft CR
.nf
\f(CRmain()
{
   \f(CBint\f(CR n, c;
 
   printf(\fB"Enter a number\\n"\f(CR);
   scanf(\fB"%d"\f(CR, &n);
 
   \f(CBif\f(CR ( n == \f52\f(CR )
      printf(\fB"Prime number.\\n"\f(CR);
   \f(CBelse
\f(CR   {
       \f(CBfor\f(CR ( c = \f52\f(CR ; c <= n - \f51\f(CR ; c++ )
       {
           \f(CBif\f(CR ( n % c == \f50\f(CR )
              \f(CBbreak\f(CR;
       }
       \f(CBif\f(CR ( c != n )
          printf(\fB"Not prime.\\n"\f(CR);
       \f(CBelse
\f(CR          printf(\fB"Prime number.\\n"\f(CR);
   }
   \f(CBreturn\f(CR \f50\f(CR;
}
